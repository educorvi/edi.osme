    "css-loader": "^5.0.1",
    "del-cli": "^3.0.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.14.0",
    "mkdirp": "^0.5.5",
    "release-it": "^14.2.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.6.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
